{
	"name": "machour/yii2-notifications",
	"description": "Yii2 Notifications",
  	"keywords": ["yii", "notification", "growl", "noty"],
	"type": "yii2-extension",
	"support": {
		"source": "https://github.com/machour/yii2-notifications"
	},
	"authors": [
		{
			"name": "Mehdi Achour",
			"email": "machour@gmail.com"
		}
	],
	"require": {
		"yiisoft/yii2": "*",
